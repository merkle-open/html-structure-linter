{
  "name": "html-structure-linter",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/namics/html-structure-linter/"
  },
  "bin": {
    "html-structure-linter": "./lib/bin.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "clean": "npx --ignore-existing --quiet rimraf package-lock.json node_modules",
    "prepare": "npm-run-all prepare:*",
    "prepare:test": "npm run test",
    "prepare:build": "npm run compile",
    "compile": "tsc",
    "test": "jest",
    "update-dependencies": "npm-check-updates -u"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "fs-extra": "9.0.1",
    "glob-all": "3.2.1",
    "minimist": "1.2.5",
    "vfile-location": "3.2.0"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.23",
    "@types/fs-extra": "9.0.5",
    "@types/jest": "26.0.19",
    "@types/minimatch": "3.0.3",
    "@types/minimist": "1.2.1",
    "@types/vfile-location": "2.0.5",
    "codelyzer": "6.0.1",
    "jest": "26.6.3",
    "npm-check-updates": "10.2.5",
    "npm-run-all": "4.1.5",
    "ts-jest": "26.4.4",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  }
}
