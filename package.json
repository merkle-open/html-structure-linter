{
  "name": "html-structure-linter",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/namics/html-structure-linter/"
  },
  "bin": {
    "html-structure-linter": "./lib/bin.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "prepare": "npm-run-all prepare:*",
    "prepare:test": "npm run test",
    "prepare:build": "npm run compile",
    "compile": "tsc",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/cheerio": "0.22.12",
    "@types/fs-promise": "1.0.3",
    "@types/jest": "24.0.15",
    "@types/minimatch": "3.0.3",
    "@types/minimist": "1.2.0",
    "codelyzer": "5.1.0",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "fs-promise": "2.0.3",
    "glob-all": "3.1.0",
    "minimist": "1.2.0",
    "vfile-location": "2.0.5"
  }
}
